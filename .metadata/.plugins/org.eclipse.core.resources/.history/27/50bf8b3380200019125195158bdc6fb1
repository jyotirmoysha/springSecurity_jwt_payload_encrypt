package com.livespread.service;

import java.util.HashMap;
import java.util.Map;

public interface ReportService {

	Map<Integer, String> getAnalystNamesByUserGroupId(Long groupId);

	HashMap<String, Long> getAllStatusForReports(Long groupId);

