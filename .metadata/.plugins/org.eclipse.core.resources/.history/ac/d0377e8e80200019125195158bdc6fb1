require("./global");config.fatal=true;global.target={};var args=process.argv.slice(2);setTimeout(function(){var e;if(args.length===1&&args[0]==="--help"){console.log("Available targets:");for(e in target)console.log("  "+e);return}for(e in target){(function(e,t){target[e]=function(e){if(t.done&&!e)return;t.done=true;return t.apply(t,arguments)}})(e,target[e])}if(args.length>0){args.forEach(function(e){if(e in target)target[e]();else{console.log("no such target: "+e);exit(1)}})}else if("all"in target){target.all()}},0)